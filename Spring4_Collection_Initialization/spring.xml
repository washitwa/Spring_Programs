<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id='Lis' class='ListCreator'>
	<property name='alist'>
	<list>
		<ref bean='point1'></ref>
		<ref bean='point2'></ref>
		<ref bean='point3'></ref>
	</list>
	</property>
	</bean>
	<bean id='point1' class='Point'>
		<property name='x' value='1'></property>
		<property name='y' value='10'></property>
	</bean>
	<bean id='point2' class='Point'>
		<property name='x' value='2'></property>
		<property name='y' value='20'></property>
	</bean>
	<bean id='point3' class='Point'>
		<property name='x' value='3'></property>
		<property name='y' value='30'></property>
	</bean>
</beans>